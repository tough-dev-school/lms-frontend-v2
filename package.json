    "eslint-plugin-vue": "^10.0.0",
    "floating-vue": "^5.2.2",
    "lodash-es": "^4.17.21",
    "minidenticons": "^4.2.1",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "prismjs": "^1.30.0",
    "turndown": "^7.2.0",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-avatar-cropper": "^6.1.1",
