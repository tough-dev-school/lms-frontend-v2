{
  "name": "lms-frontend-v2",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --config ./tests/unit.config.ts",
    "test:unit:coverage": "vitest --coverage --config ./tests/unit.config.ts",
    "test:visual": "concurrently --success first --kill-others \"start-storybook -p 3000\" \"wait-on http://localhost:3000 && vitest --config ./tests/visual.config.ts\"",
    "test:visual:update": "concurrently --success first --kill-others \"start-storybook -p 3000\" \"wait-on http://localhost:3000 && vitest --update --config ./tests/visual.config.ts\"",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --max-warnings=0",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@sentry/tracing": "^7.22.0",
    "@sentry/vue": "^7.22.0",
    "@tiptap/extension-bubble-menu": "^2.0.0-beta.209",
    "@tiptap/extension-placeholder": "^2.0.0-beta.209",
    "@tiptap/starter-kit": "^2.0.0-beta.199",
    "@tiptap/vue-3": "^2.0.0-beta.199",
    "@vueuse/core": "^9.2.0",
    "axios": "^0.27.2",
    "camelcase-keys": "^8.0.2",
    "dayjs": "^1.11.5",
    "decamelize-keys": "^2.0.1",
    "floating-vue": "^2.0.0-beta.20",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "pinia": "^2.0.21",
    "pinia-plugin-persistedstate": "^2.2.0",
    "prosemirror-commands": "^1.5.0",
    "prosemirror-dropcursor": "^1.6.1",
    "prosemirror-gapcursor": "^1.3.1",
    "prosemirror-history": "^1.3.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.18.3",
    "prosemirror-schema-list": "^1.2.2",
    "prosemirror-state": "^1.4.2",
    "prosemirror-transform": "^1.7.0",
    "prosemirror-view": "^1.29.1",
    "turndown": "^7.1.1",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5",
    "vue-tabler-icons": "^2.15.0",
    "vue3-notion": "^0.1.36"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@faker-js/faker": "^7.5.0",
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.12",
    "@tailwindcss/typography": "^0.5.7",
    "@types/jest-image-snapshot": "^5.1.0",
    "@types/jsdom": "^20.0.0",
    "@types/lodash": "^4.14.189",
    "@types/node": "^16.11.56",
    "@types/turndown": "^5.0.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitest/coverage-istanbul": "^0.25.8",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.11",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.5.0",
    "eslint": "^8.22.0",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-vue": "^9.3.0",
    "jest-image-snapshot": "^5.2.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.27.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "serve": "^14.0.1",
    "storybook-vue3-router": "^2.3.1",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vitest": "^0.23.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.40.7",
    "wait-on": "^6.0.1"
  }
