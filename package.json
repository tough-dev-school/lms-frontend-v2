{
  "name": "lms-frontend-v2",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p check:type build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --config ./tests/unit.config.ts",
    "test:unit:coverage": "vitest --coverage --config ./tests/unit.config.ts",
    "test:visual": "concurrently --success first --kill-others \"pnpm run storybook\" \"wait-on http://localhost:3000 && vitest --config ./tests/visual.config.ts\"",
    "test:visual:update": "concurrently --success first --kill-others \"pnpm run storybook\" \"wait-on http://localhost:3000 && vitest --update --config ./tests/visual.config.ts\"",
    "build-only": "vite build",
    "check:type": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "check:lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --max-warnings=0",
    "fix:lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --max-warnings=0",
    "check:format": "prettier --write .",
    "check": "run-p fix:*",
    "fix": "run-p fix:*",
    "storybook": "storybook dev -p 3000",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sentry/tracing": "^7.55.2",
    "@sentry/vue": "^7.55.2",
    "@tiptap/extension-bubble-menu": "^2.0.3",
    "@tiptap/extension-image": "^2.0.3",
    "@tiptap/extension-placeholder": "^2.0.3",
    "@tiptap/pm": "^2.0.3",
    "@tiptap/starter-kit": "^2.0.3",
    "@tiptap/vue-3": "^2.0.3",
    "@vueuse/core": "^9.13.0",
    "axios": "^0.27.2",
    "camelcase-keys": "^8.0.2",
    "dayjs": "^1.11.8",
    "decamelize-keys": "^2.0.1",
    "eslint-plugin-vue": "^9.14.1",
    "floating-vue": "2.0.0-beta.20",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^2.4.0",
    "prismjs": "^1.29.0",
    "turndown": "^7.1.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue-tabler-icons": "^2.21.0",
    "vue3-notion": "github:tough-dev-school/vue3-notion"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@faker-js/faker": "^7.6.0",
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.3.2",
    "@storybook/addon-actions": "^7.0.21",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/addon-interactions": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/vue3": "^7.0.21",
    "@storybook/vue3-vite": "^7.0.21",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest-image-snapshot": "^5.1.0",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^16.18.36",
    "@types/turndown": "^5.0.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-istanbul": "^0.25.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.42.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "jest-image-snapshot": "^5.2.0",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.35.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.21",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.7.4",
    "vite": "^3.2.7",
    "vitest": "^0.23.4",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.40.13",
    "wait-on": "^6.0.1"
  }
}
